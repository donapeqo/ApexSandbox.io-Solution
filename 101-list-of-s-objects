public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here


    List<sObject> accAndCon = new List<sObject>();

    if(accounts != null){
    accAndCon.addAll(accounts);
    }
    if(contacts != null){
    accAndCon.addAll(contacts);    
    }

    return accAndCon;
    
}