public String registerStudent(String name, String phone, String email) {
    apxio__Student__c std = new apxio__Student__c(
    Name = name;
    apxio__Phone__c = phone;
    apxio__Email__c = email;
    );
    
    insert std;

    List<apxio__Student__c>apxio__Student__c astd = [SELECT Id,apxio__Registration_Number__c  FROM apxio__Student__c WHERE id = :std.Id];
    
    return astd[0].apxio__Registration_Number__c;
}